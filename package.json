  "license": "MIT",
  "scripts": {
    "dev": "ts-node --project tsconfig.server.json server.ts -- --inspect",
    "debug": "ts-node --project tsconfig.server.json server.ts",
    "lint": "stylelint \"**/*.{css,scss}\" --fix --ignore-path .stylelintignore && eslint --ext js,ts --fix --ignore-path .eslintignore . ; exit 0",
    "tinker": "concurrently \"yarn lint\" \"yarn dev\"",
    "build:server": "tsc --project tsconfig.server.json",
    "build:next": "next build",
    "start": "NODE_ENV=production node server.js",
    "deploy": "yarn build:next && yarn start",
    "analyze": "ANALYZE=true yarn build",
    "sitemap": "node sitemap_generator.js",
    "test": "TZ=Asia/Jakarta jest --coverage --colors --config=jest.config.js -u --watchAll --silent",
    "test:watch": "TZ=Asia/Jakarta jest --colors --config=jest.config.js -u --watchAll --silent",
    "test:e2e": "jest --colors --config=jest.config.js -u --watchAll ./pages",
    "test:ci": "TZ=Asia/Jakarta jest --coverage --colors --config=jest.config.ci.js -u --silent"
  },
